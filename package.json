{
  "name": "gw-boilerplate",
  "version": "1.0.0",
  "description": "centering art",
  "main": "index.js",
  "scripts": {
    "start": "npm run watch",
    "watch": "cross-env NODE_ENV=development node index",
    "build": "npm run build:client && npm run build:server",
    "serve": "npm run build && cross-env NODE_ENV=production node index",
    "build:client": "cross-env NODE_ENV=production webpack -p --config webpack/client.prod.js",
    "build:server": "cross-env NODE_ENV=production webpack -p --config webpack/server.prod.js"
